{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "src/api/progress.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/progress",
      "dest": "/api/progress.js"
    },
    {
      "src": "/api/slack/(.*)",
      "dest": "https://slack.com/$1"
    },
    {
      "src": "/callback",
      "dest": "/"
    }
  ]
}